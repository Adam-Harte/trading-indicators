{
  "name": "ti-js",
  "version": "0.0.1",
  "description": "package for calculating trading indicators",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint **/*.js --ignore-pattern node_modules/",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write \"src/**/*.{ts,json}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,json}\"",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Adam-Harte/trading-indicators.git"
  },
  "author": "Adam Harte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Adam-Harte/trading-indicators/issues"
  },
  "homepage": "https://github.com/Adam-Harte/trading-indicators#readme",
  "devDependencies": {
    "@types/jest": "^28.1.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "round-to": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.test.ts": [
      "npm run test"
    ],
    "src/**/*.ts": ["npm run lint", "npm run prettier"]
  }
}
